<launch>
  <node pkg="controller_pkg"
      type="controller_node.py"
      name="controller_node"
      output="screen">

  <param name="image_topic"   value="/B1/rrbot/camera1/image_raw" />
  <param name="cmd_vel_topic" value="/B1/cmd_vel" />

    </node>

    <node pkg="image_view"
        type="image_view"
        name="B1_camera_view"
        output="screen">
    <remap from="image" to="/B1/rrbot/camera1/image_raw" />
    </node>
</launch>
